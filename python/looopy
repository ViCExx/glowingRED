foods = ["stirfry", "tortillas", "burgers", "tacos", "pizza"]

for i in range(10):
    print(i, end=" ") # numbers 0 - 9
print()

for food in foods:
    print(food, end=" ") # elements through a collections
print()

print(len(foods))
for i in range(len(foods)): # through elements, with numbers paired with it  
    print(i, foods[i], end=" \n") # added \n to break out list
print()